# FashionOS Environment Configuration

# Application
NEXT_PUBLIC_APP_NAME=FashionOS
NEXT_PUBLIC_APP_URL=http://localhost:3000
# PORT=3000  # Uncomment to lock port, otherwise auto-selects 3000-3010

# Supabase (Already configured in .env.local)
# NEXT_PUBLIC_SUPABASE_URL=your-supabase-url
# NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Features
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_REALTIME=true
NEXT_PUBLIC_ENABLE_COMMAND_PALETTE=true

# Development
REFINE_DEVTOOLS_PORT=5001